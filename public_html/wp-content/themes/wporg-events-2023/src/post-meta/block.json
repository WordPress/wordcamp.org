{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wporg/post-meta",
	"version": "0.1.0",
	"title": "WordPress Post Meta",
	"category": "design",
	"icon": "list-view",
	"description": "Displays Post Meta",
	"textdomain": "wporg",
	"attributes": {
		"id": {
			"type": "integer"
		},
		"key": {
			"type": "string",
			"default": "",
			"required": true
		},
		"single": {
			"type": "boolean",
			"default": true
		}
	},
	"editorScript": "file:./index.js"
}
